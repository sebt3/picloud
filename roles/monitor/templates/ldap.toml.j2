[[servers]]
host = "ldap"
port = 389
use_ssl = false
start_tls = false
ssl_skip_verify = false
bind_dn = "cn=admin,{{ ldap_root }}"
bind_password = '{{ admin_password }}'
search_filter = "(uid=%s)"
search_base_dns = ["{{ ldap_base }}"]

[servers.attributes]
name = "givenName"
surname = "sn"
username = "uid"
member_of = "memberOf"
email =  "mail"

